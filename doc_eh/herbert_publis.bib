	journal = {Review of Scientific Instruments},
	author = {Rousset, B. and Bonnay, P. and Diribarne, P. and Girard, A. and Poncet, J. M. and Herbert, E. and Salort, J. and Baudet, C. and Castaing, B. and Chevillard, L. and Daviaud, F. and Dubrulle, B. and Gagne, Y. and Gibert, M. and Hébral, B. and Lehner, Th. and Roche, P.-E. and Saint-Michel, B. and Bon Mardion, M.},
	month = oct,
	year = {2014},
	pages = {103908},
