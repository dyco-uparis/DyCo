	pages = {015103},
	file = {Full Text PDF:/home/eh/Zotero/storage/W6ZG8P78/Herbert et al. - 2014 - Eckhaus-like instability of large scale coherent s.pdf:application/pdf;Snapshot:/home/eh/Zotero/storage/AXAGXVKI/1.html:text/html}
}

